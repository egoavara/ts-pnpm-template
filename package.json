{
    "name": "ts-pnpm-template",
    "version": "0.0.0",
    "description": "",
    "type": "module",
    "types": "./types/index.d.ts",
    "main": "./cjs/index.js",
    "module": "./esm/index.js",
    "exports": {
        ".": {
            "import": "./esm/index.js",
            "types": "./types/index.d.ts",
            "require": "./cjs/index.js"
        }
    },
    "files": [
        "types/**/*.d.ts",
        "cjs/**/*.js",
        "cjs/package.json",
        "esm/**/*.js",
        "esm/package.json"
    ],
    "scripts": {
        "build": "rimraf \"{cjs,esm,types}/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc -b ./cjs/tsconfig.json ./esm/tsconfig.json ./types/tsconfig.json",
        "watch": "rimraf \"{cjs,esm,types}/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc -b ./cjs/tsconfig.json ./esm/tsconfig.json ./types/tsconfig.json --watch",
        "postversion": "npm publish",
        "prepack": "npm run build",
        "setup-test": "docker compose -f ./test/docker-compose.yml -p fastify-modular-pg up -d ",
        "test": "tap --node-arg=--loader=ts-node/esm ./tests/*.ts"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^18.0.5",
        "@types/tap": "^15.0.7",
        "@typescript-eslint/eslint-plugin": "^5.30.7",
        "@typescript-eslint/parser": "^5.30.7",
        "eslint": "^8.20.0",
        "rimraf": "^3.0.2",
        "tap": "^16.3.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    }
}